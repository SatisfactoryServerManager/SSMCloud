            referrerpolicy="no-referrer"
        ></script>
        <script
            defer
            src="/public/libs/jquery-steps/jquery.steps.min.js"
        ></script>

        <script
            defer
            src="https://unpkg.com/leaflet@1.9.4/dist/leaflet.js"
            integrity="sha256-20nQCchB9co0qIjJZRGuk2/Z9VM+kNiyxNV1lvTlZBo="
            crossorigin=""
        ></script>

        <script defer src="/public/js/bundle.js"></script>

        <script>
            // Pass Go flash messages into JS safely as JSON
            window.flashes = {{ toJson .flashes }};

            window.displayFlashes = () => {
                if (window.flashes && window.flashes.length > 0) {
                    const options={timeOut:3000, "closeButton": true,"progressBar": true,"positionClass": "toast-bottom-right",}
                    window.flashes.forEach(flash => {
                        switch (flash.type) {
                            case "success": toastr.success(flash.message,"",options); break;
                            case "error": toastr.error(flash.message,"",options); break;
                            case "warning": toastr.warning(flash.message,"",options); break;
                            default: toastr.info(flash.message,"",options);
                        }
                    });
                }
            }
        </script>

        <meta name="HandheldFriendly" content="True" />
        <meta
            name="viewport"
            content="width=device-width, initial-scale=1.0, viewport-fit=cover"
        />

        <link rel="apple-touch-icon" sizes="180x180" href="/public/images/favicons/apple-touch-icon.png">
