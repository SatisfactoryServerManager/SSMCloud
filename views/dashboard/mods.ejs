<%- include('../includes/head-main.ejs') %>

<body>
  <div id="viewport" class="minimal">
    <%- include('../includes/navigation.ejs') %>
    <div id="content">
      <div class="container-fluid">
        <!-- BODY -->
        <div class="page-container" data-page="dashboard">
          <%- include('../includes/page-header.ejs') %>

          <div class="row">
            <%agents.forEach(agent=>{%>
            <%- include('../includes/mods-agent.ejs',{agent, mods}) %>
            <%})%>
          </div>
        </div>
      </div>

      <%- include('../includes/footer-main.ejs') %>
    </div>
  </div>
</body>


<script>
  let mods = <%- JSON.stringify(mods) %>;
  localStorage.setItem('mods', JSON.stringify({
    'mods': mods
  }));
</script>